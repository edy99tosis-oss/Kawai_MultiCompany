                        !Last_Update = Now
                        !last_user = userLogin
                        .update
                    End If
                    lotno = !Lot_no
                End With
                rst.Close
                Set rst = Nothing
            End If
        Next
    Next
    rslot.Close
    Set rslot = Nothing
    dbz.Close
    Set dbz = Nothing
    If getdata Then displayrecords
    If blnupdate Then lblerror = DisplayMsg(1000)

End Sub

