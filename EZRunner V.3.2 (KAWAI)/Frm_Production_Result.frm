Private Function getdata() As Boolean
    
    Dim sqlP As String
    
    If Trim(cbodealer.Text) = "" Then Exit Function
    If Trim(cboGroup.Text) = "" Then Exit Function
    If Trim(CboPart.Text) = "" Then Exit Function
    
    If Trim(cboGroup) = strAll Then
        sqlP = ") x "
    Else
        sqlP = "and item_master.group_cls = '" & cboGroup.Text & "') x "
    End If
    
    sql = "     select x.*,isnull(aqty,0) r1,isnull(bqty,0) r2,isnull(cqty,0) r3, isnull(dqty,0) r4,isnull(eqty,0) r5,isnull(fqty,0) r6,  " & vbCrLf & _
            "   isnull(shipment1.qty,0) Shipment1, isnull(shipment2.qty,0) Shipment2, isnull(shipment3.qty,0) Shipment3, " & vbCrLf & _
            "   isnull(shipment4.qty,0) Shipment4, isnull(shipment5.qty,0) Shipment5, isnull(shipment6.qty,0) Shipment6 from " & vbCrLf
    
    sql = sql + "(select  isnull(a.qty,0) bln1, isnull(b.qty,0) bln2, isnull(c.qty,0)bln3," & vbCrLf & _
                    "isnull(d.qty,0)bln4, isnull(e.qty,0)bln5, isnull(f.qty,0)bln6 , item_master.item_Code, " & vbCrLf & _
                    "rtrim(item_master.makeritem_code) makercode, rtrim(item_master.item_name) itemname ,item_master.unit_cls " & vbCrLf
                 
    sql = sql + "  From Item_Master Left Join (select item_Code, qty from production_planning where prod_year = '" & thn(0) & "' and prod_month = '" & bln(0) & "') a on Item_Master.Item_Code=a.Item_Code " & vbCrLf & _
                    " Left Join (select item_Code, qty from production_planning where prod_year = '" & thn(1) & "' and prod_month = '" & bln(1) & "') b on Item_Master.Item_Code=b.Item_Code  " & vbCrLf & _
                    " Left Join (select item_Code, qty from production_planning where prod_year = '" & thn(2) & "' and prod_month = '" & bln(2) & "') c on Item_Master.Item_Code=c.Item_Code  " & vbCrLf & _
                    " Left Join (select item_Code, qty from production_planning where prod_year = '" & thn(3) & "' and prod_month = '" & bln(3) & "') d on Item_Master.Item_Code=d.Item_Code  " & vbCrLf & _
                    " Left Join (select item_Code, qty from production_planning where prod_year = '" & thn(4) & "' and prod_month = '" & bln(4) & "') e on Item_Master.Item_Code=e.Item_Code  " & vbCrLf & _
                    " Left Join (select item_Code, qty from production_planning where prod_year = '" & thn(5) & "' and prod_month = '" & bln(5) & "') f on Item_Master.Item_Code=f.Item_Code  " & vbCrLf & _
                    " Left Join trade_master On trade_master.trade_code = item_master.manufacture_code " & vbCrLf & _
                " Where 'A'='A' " & vbCrLf

    
    If Trim(CboPart) <> strAll Then
        sql = sql + " and item_master.item_code >= '" & CboPart & "' "
    End If
    
    If Trim(cbodealer.Text) = strAll Then
        sql = sql + " and finishgoodpart_cls = '01' and production_cls='01' " & sqlP
    Else
        sql = sql + " and item_master.manufacture_code= '" & cbodealer & "' and finishgoodpart_cls = '01' and production_cls='01' " & sqlP
    End If
' ' Untuk KAWAI tidak hanya FG
'    If Trim(cbodealer.Text) = strAll Then
'        Sql = Sql + " and production_cls='01' " & sqlP
'    Else
'        Sql = Sql + " and item_master.manufacture_code= '" & cbodealer & "' and finishgoodpart_cls = '01' and production_cls='01' " & sqlP
'    End If
    
' -------------------
    sql = sql + " Left Join (select item_code, sum(qty) aqty from orderEntry_detail where  year(delivery_date) ='" & thn(0) & "' and month(delivery_date) = '" & bln(0) & "'  group by item_code) ra on x.item_code = ra.item_code " & vbCrLf & _
                    " Left Join (select item_code, sum(qty) bqty from orderEntry_detail where  year(delivery_date) = '" & thn(1) & "' and month(delivery_date) = '" & bln(1) & "' group by item_code) rb on x.item_code = rb.item_code  " & vbCrLf & _
                    " Left Join (select item_code, sum(qty) cqty from orderEntry_detail where  year(delivery_date) = '" & thn(2) & "' and month(delivery_date) = '" & bln(2) & "' group by item_code) rc on x.item_code = rc.item_code  " & vbCrLf & _
                    " Left Join (select item_code, sum(qty) dqty from orderEntry_detail where  year(delivery_date) = '" & thn(3) & "' and month(delivery_date) = '" & bln(3) & "' group by item_code) rd on x.item_code = rd.item_code  " & vbCrLf & _
                    " Left Join (select item_code, sum(qty) eqty from orderEntry_detail where  year(delivery_date) = '" & thn(4) & "' and month(delivery_date) = '" & bln(4) & "'  group by item_code) re on x.item_code = re.item_code  " & vbCrLf & _
                    " Left Join (select item_code, sum(qty) fqty from orderEntry_detail where  year(delivery_date) = '" & thn(5) & "' and month(delivery_date) = '" & bln(5) & "'  group by item_code) rf on x.item_code = rf.item_code  " & vbCrLf
                    
    sql = sql + "   Left Join (select item_code , sum(qty) qty from do_master dm, delivery_order do where dm.do_no = do.do_no and year(do_date) ='" & thn(0) & "' and month(do_date) ='" & bln(0) & "' and fix_cls ='1' group by item_code) Shipment1 on x.item_code = Shipment1.item_code  " & vbCrLf & _
                "   Left Join (select item_code , sum(qty) qty from do_master dm, delivery_order do where dm.do_no = do.do_no and year(do_date) ='" & thn(1) & "' and month(do_date) ='" & bln(1) & "' and fix_cls ='1' group by item_code) Shipment2 on x.item_code = Shipment2.item_code " & vbCrLf & _
                "   Left Join (select item_code , sum(qty) qty from do_master dm, delivery_order do where dm.do_no = do.do_no and year(do_date) ='" & thn(2) & "' and month(do_date) ='" & bln(2) & "' and fix_cls ='1' group by item_code) Shipment3 on x.item_code = Shipment3.item_code " & vbCrLf & _
                "   Left Join (select item_code , sum(qty) qty from do_master dm, delivery_order do where dm.do_no = do.do_no and year(do_date) ='" & thn(3) & "' and month(do_date) ='" & bln(3) & "' and fix_cls ='1' group by item_code) Shipment4 on x.item_code = Shipment4.item_code " & vbCrLf & _
                "   Left Join (select item_code , sum(qty) qty from do_master dm, delivery_order do where dm.do_no = do.do_no and year(do_date) ='" & thn(4) & "' and month(do_date) ='" & bln(4) & "' and fix_cls ='1' group by item_code) Shipment5 on x.item_code = Shipment5.item_code " & vbCrLf & _
                "   Left Join (select item_code , sum(qty) qty from do_master dm, delivery_order do where dm.do_no = do.do_no and year(do_date) ='" & thn(5) & "' and month(do_date) ='" & bln(5) & "' and fix_cls ='1' group by item_code) Shipment6 on x.item_code = Shipment6.item_code " & vbCrLf
                
    sql = sql + "   order by x.item_Code "
    
    Set rstresult = New ADODB.Recordset
    rstresult.Open sql, Db, adOpenKeyset
    rstresult.PageSize = 5
    page = 0
    getdata = True

End Function

Private Sub displayrecords()

    Dim rstime As New ADODB.Recordset
    
    totalPage = rstresult.PageCount
    If page > totalPage Then page = totalPage
    If page > 0 Then rstresult.AbsolutePage = page
    
    clear
    
    If rstresult.EOF = False Then
        For Y = 0 To 4
            If page = 0 Then page = 1
            If Not rstresult.EOF Then
                lblitem(Y) = Trim(rstresult!MakerCode) & "/" & Trim(rstresult!Item_Code)
                lblitem(Y).Tag = Trim(rstresult!Item_Code)
                Me.Descitem(Y) = Trim(rstresult!itemname)
                For i = 0 To 5
                    Text1(i + Y * 6).Enabled = True
                    'Forecast data
                    If InStr(1, rstresult.Fields(i).Value, ".") Then
                        Text1(i + Y * 6).Text = Format(rstresult.Fields(i).Value, gs_formatQty)
                    Else
                        Text1(i + Y * 6).Text = Format(rstresult.Fields(i).Value, gs_formatQty)
                    End If
                    'Order data
                    If InStr(1, rstresult.Fields(i + 10).Value, ".") Then
                        Text2(i + Y * 6).Text = Format(rstresult.Fields(i + 10).Value, gs_formatQty)
                    Else
                        Text2(i + Y * 6).Text = Format(rstresult.Fields(i + 10).Value, gs_formatQty)
                    End If
                    'Shipment data
                    If InStr(1, rstresult.Fields(i + 16).Value, ".") Then
                        Text3(i + Y * 6).Text = Format(rstresult.Fields(i + 16).Value, gs_formatQty)
                    Else
                        Text3(i + Y * 6).Text = Format(rstresult.Fields(i + 16).Value, gs_formatQty)
                    End If
                    
                    'Balance Order - Shipment
                    If InStr(1, CDbl(Text1(i + Y * 6).Text) - CDbl(Text2(i + Y * 6).Text), ".") Then
                        Text4(i + Y * 6).Text = Format(CDbl(Text2(i + Y * 6).Text) - CDbl(Text3(i + Y * 6).Text), gs_formatQty)
                    Else
                        Text4(i + Y * 6).Text = Format(CDbl(Text2(i + Y * 6).Text) - CDbl(Text3(i + Y * 6).Text), gs_formatQty)
                    End If
                    
                    Text1(i + Y * 6).Tag = rstresult!Unit_cls & "," & CDbl(Text1(i + Y * 6))
                    Text1(i + Y * 6).BackColor = vbWhite
                    Text1(i + Y * 6).DataChanged = False
                    Text2(i + Y * 6).DataChanged = False
                    Text3(i + Y * 6).DataChanged = False
                    Text4(i + Y * 6).DataChanged = False
                Next
            rstresult.MoveNext
            End If
        Next
    End If
    
    If page < 0 Then page = 0
    If totalPage < 0 Then totalPage = 0
    lblpage.Caption = "Page " & page & " of " & totalPage
    rstime.Open "select Last_Update from production_planning order by Last_Update desc", Db, adOpenDynamic, adLockOptimistic
    If Not rstime.EOF Then
        Label3.Caption = Format(rstime!Last_Update, "dd mmm yyyy hh:mm:ss")
    Else
        Label3.Caption = ""
    End If
    rstime.Close
    Set rstime = Nothing

End Sub

